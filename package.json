{
  "name": "jarvis-bot",
  "version": "0.1.6",
  "author": "kengz",
  "description": "A JARVIS for your team - your most powerful bot and loyal butler.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "stop": "forever stopall",
    "test": "./bin/hubot -a slack",
    "jarvis": "forever start --minUptime=1000 --spinSleepTime=1000 --killSignal=SIGTERM -a -l jarvis.log --uid 'jarvis' -m 10 run.js",
    "veronica": "forever start --minUptime=1000 --spinSleepTime=1000 --killSignal=SIGTERM -a -l veronica.log --uid 'veronica' -m 10 run.js"
  },
